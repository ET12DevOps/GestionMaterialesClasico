<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    <!-- link href="~/Content/Site.css" rel="stylesheet" type="text/css" / -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Oficina Técnica", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Entrada<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Orden de Trabajo de Aplicación", "Index", "OrdenTrabajo")</li>
                            <li>@Html.ActionLink("Orden de Pedido", "Index", "OrdenPedido")</li>
                            <li>@Html.ActionLink("Trabajo Práctico", "", "")</li>
                            <li>@Html.ActionLink("Donación", "", "")</li>
                            <li>@Html.ActionLink("Sobrante", "", "")</li>
                        </ul>
                    </li>
                    <li>@Html.ActionLink("Stock", "Index", "Stock")</li>
                    <li>@Html.ActionLink("Materiales", "Index", "Material")</li>
                    <li>@Html.ActionLink("Proveedores", "Index", "Proveedor")</li>
                    <li>@Html.ActionLink("Personal", "Index", "Personal")</li>
                    </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        <br />
            @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - ET N°12 DE 1 Libertador General José de San Martín</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>
